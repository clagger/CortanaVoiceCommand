<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="WorldpopulationCommandSet_en-us">
    <CommandPrefix> Listen up </CommandPrefix>
    <Example> show me the population of ghana in 2006 </Example>

    <Command Name="showPastPopulation">
      <Example>  show me the population of ghana in 2006 </Example>
      <ListenFor> show [me] the population of [the] {country} in {year} </ListenFor>
      <ListenFor> how big was the population in {country} in {year} </ListenFor>
      <Feedback> Showing population of {country} in {year} </Feedback>
      <VoiceCommandService Target="VoiceCommandService"/>
    </Command>

    <Command Name="showPastWomenPercentage">
      <Example>How high was the proportion of women in Afghanistan in 2009?</Example>
      <ListenFor>how high was the proportion of women in {country} in {year}</ListenFor>
      <Feedback> Showing the proportion of women in {country} in {year} </Feedback>
      <VoiceCommandService Target="VoiceCommandService"/>
    </Command>

    <Command Name="showFuturePopulation">
      <Example> How high will the population of Afghanistan be in 2020? </Example>
      <ListenFor>How high will the population of {country} be in {year}</ListenFor>
      <ListenFor>How big will the population of {country} be in {year}</ListenFor>
      <Feedback> Showing the population of {country} in {year} </Feedback>
      <VoiceCommandService Target="VoiceCommandService"/>
    </Command>

